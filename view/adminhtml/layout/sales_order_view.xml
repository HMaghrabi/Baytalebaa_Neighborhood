<?xml version="1.0"?>
<!--
/**
 * Layout configuration for customer neighborhood order info
 *
 * @category  Baytalebaa
 * @package   Baytalebaa_CustomerNeighborhood
 */
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <!-- Override the default order info template -->
        <referenceBlock name="order_info">
            <action method="setTemplate">
                <argument name="template"
                          xsi:type="string">Baytalebaa_CustomerNeighborhood::order/view/info.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>